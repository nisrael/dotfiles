# Btop carbonfox theme
# Based on nightfox.nvim carbonfox palette

# Main background, empty for terminal default
theme[main_bg]="#161616"

# Main text color
theme[main_fg]="#f2f4f8"

# Title color for boxes
theme[title]="#f2f4f8"

# Highlight color for keyboard shortcuts
theme[hi_fg]="#78a9ff"

# Background color of selected item in processes box
theme[selected_bg]="#2a2a2a"

# Foreground color of selected item in processes box
theme[selected_fg]="#f2f4f8"

# Color of inactive/disabled text
theme[inactive_fg]="#7b7c7e"

# Color of text appearing on top of graphs, i.e uptime and b/s
theme[graph_text]="#b6b8bb"

# Misc colors for processes box including mini cpu graphs, details memory graph and details status text
theme[proc_misc]="#33b1ff"

# Cpu box outline color
theme[cpu_box]="#535353"

# Memory/disks box outline color
theme[mem_box]="#535353"

# Net up/down box outline color
theme[net_box]="#535353"

# Processes box outline color
theme[proc_box]="#535353"

# Box divider line and small boxes line color
theme[div_line]="#353535"

# Temperature graph colors
theme[temp_start]="#25be6a"
theme[temp_mid]="#08bdba"
theme[temp_end]="#ee5396"

# CPU graph colors
theme[cpu_start]="#25be6a"
theme[cpu_mid]="#08bdba"
theme[cpu_end]="#78a9ff"

# Mem/Disk free meter
theme[free_start]="#25be6a"
theme[free_mid]=""
theme[free_end]=""

# Mem/Disk cached meter
theme[cached_start]="#33b1ff"
theme[cached_mid]=""
theme[cached_end]=""

# Mem/Disk available meter
theme[available_start]="#be95ff"
theme[available_mid]=""
theme[available_end]=""

# Mem/Disk used meter
theme[used_start]="#ee5396"
theme[used_mid]=""
theme[used_end]=""

# Download graph colors
theme[download_start]="#33b1ff"
theme[download_mid]="#78a9ff"
theme[download_end]="#be95ff"

# Upload graph colors
theme[upload_start]="#25be6a"
theme[upload_mid]="#08bdba"
theme[upload_end]="#3ddbd9"

# Process box color gradient for threads, mem and cpu usage
theme[process_start]="#78a9ff"
theme[process_mid]="#be95ff"
theme[process_end]="#ee5396"
